*{
    margin: 0;
    padding: 0;
}

/* CSS thành phần menuWeb */
.menuWeb{
    width: 98%;
    margin-bottom: 10px;
    background-color: #33FF99;
}                   

/* CSS thành phần homeWeb */
.homeWeb{
    width: 99%;
    margin:  10px auto 10px auto;
}
.sidebarWeb{
    width: 100%;
    margin-bottom: 10px;
}
.inconHome{
    color: red;
    
}
.itemSidebarWeb{
    margin-bottom: 5px;
}
.itemSidebarWeb button{
    width: 100%; 
    text-align: left;
    height: 50px;
    font-size: 16px;
    font-weight: bold;
    background-color: #33FF99;  
    /* #33FF99 */
}
.itemSidebarWeb ul{
    width: 100%; 
    font-size: 16px;
}
.mainWeb{
    width: 99%;
    background-color: #F5F5F5;  
}
.fromSearchAdmin{
    width: 300px;

}
.formThemNguoiDung{
    margin: auto;
    background-color: #F5F5F5; 
    height: 500px; 
    width: 1000px; 
    border-radius: 2%;
}
.avt-background{
    width: 100%;
    height: 100px;
    background: #b6ead0;
}


/* CSS thành phần footer */
.footerWeb{
    width: 100%;
    height: 200px;
    background-color: #F0FFF0;
}

/* CSS thành phần phụ */
.navbar-nav li a{
    color: white;
    font-size: 20px;
}
